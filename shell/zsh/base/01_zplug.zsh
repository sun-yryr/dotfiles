source $HOME/.zplug/init.zsh

# ------------------------------------------------------------
# プラグイン定義
# ------------------------------------------------------------
zplug "b4b4r07/enhancd", use:init.sh



# ------------------------------------------------------------
# プラグインのインストールと読み込み
# ------------------------------------------------------------
if ! zplug check; then
    printf "Install? [y/N]: "
    if read -q; then
        echo; zplug install
    fi
fi
zplug load
